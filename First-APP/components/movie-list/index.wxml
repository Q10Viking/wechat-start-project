<view class="container f-class">
  <view class="title-container">
      <text>{{title}}</text>
      <text class="more-text" bind:tap="onJumpToMoreMovie">更多></text>
  </view>
  <view class="movie-container">
    <block wx:for="{{movies}}" wx:key="id" wx:for-item="movie">
      <movie movie="{{movie}}"/>
    </block>
  </view>

</view>